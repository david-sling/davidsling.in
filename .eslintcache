Message = () => setSuccessMessage(null);\n\n  useEffect(() => {\n    setPageNow(\"CONTACT\");\n    analytics.logEvent(\"event\", \"page_view\", {\n      page_path: \"/contact\",\n      page_title: \"CONTACT\",\n    });\n  }, []);\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    if (formData.ph.length < 8) return alert(\"Invalid phone number\");\n    await firestore.add(\"messages\", formData);\n    setSuccessMessage(\"Message sent! Thanks for showing interest.\");\n    setFormData({\n      name: \"\",\n      ph: \"\",\n      email: \"\",\n      message: \"\",\n    });\n    analytics.logEvent(\"event\", \"screen_view\", {\n      screen_name: \"Contact info submitted\",\n    });\n    push(\"/contact?requested=true\");\n  };\n\n  return (\n    <>\n      <div className=\"Contact\">\n        <div className=\"circle one\"></div>\n        <div className=\"circle two\"></div>\n        <form action=\"submit\" onSubmit={handleSubmit}>\n          <h1>Lets's get in touch!</h1>\n          <TextField\n            label=\"Name\"\n            required\n            variant=\"filled\"\n            value={formData.name}\n            onChange={({ target: { value } }) =>\n              setFormData({ ...formData, name: value })\n            }\n          />\n          <MuiPhoneNumber\n            name=\"phone\"\n            required\n            label=\"Phone Number\"\n            data-cy=\"user-phone\"\n            defaultCountry={\"in\"}\n            variant=\"filled\"\n            value={formData.ph}\n            onChange={(value) => setFormData({ ...formData, ph: value })}\n          />\n          <TextField\n            label=\"E-mail\"\n            required\n            type=\"email\"\n            variant=\"filled\"\n            value={formData.email}\n            onChange={({ target: { value } }) =>\n              setFormData({ ...formData, email: value })\n            }\n          />\n          <TextField\n            label=\"Message\"\n            multiline\n            rows={5}\n            variant=\"filled\"\n            value={formData.message}\n            onChange={({ target: { value } }) =>\n              setFormData({ ...formData, message: value })\n            }\n          />\n          <button\n            id=\"button\"\n            className={successMessage && \"sent\"}\n            type=\"submit\"\n          >\n            {successMessage ? \"SENT\" : \"SUBMIT\"}\n          </button>\n          <div className=\"details\">\n            {contact.map((item) => (\n              <a href={item.link} target=\"_blank\">\n                <h4>{item.content}</h4>\n              </a>\n            ))}\n          </div>\n        </form>\n      </div>\n      <Alert\n        onClose={closeSuccessMessage}\n        severity=\"success\"\n        open={successMessage}\n      >\n        {successMessage}\n      </Alert>\n    </>\n  );\n}\n","/Users/davidsling/Documents/GitHub/davidsling.in/src/config/nav.js",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/config/contact.js",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Home/Footer.jsx",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Home/Skills.jsx",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Home/Works.jsx",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Alert.jsx",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Home/Hero.jsx",["100"],"/Users/davidsling/Documents/GitHub/davidsling.in/src/services/firebase/index.js",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/config/skills.js",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/config/works.js",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Home/WorkCard.jsx",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Home/SkillCard.jsx",[],"/Users/davidsling/Documents/GitHub/davidsling.in/src/components/Home/LogoAnim.jsx",[],"/Users/davidsling/Documents/GitHub/davidslin